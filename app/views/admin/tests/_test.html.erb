<tr>
  <td><%= test.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.category.title %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to t('.open'), admin_test_path(test), class: 'btn btn-primary' %></td>
  <td><%= link_to t('.edit'), edit_admin_test_path(test), class: 'btn btn-warning' %></td>
  <td><%= button_to t('.delete'), admin_test_path(test), method: :delete, class: 'btn btn-danger' %></td>    
</tr>
